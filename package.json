{
  "packageManager": "pnpm@9.14.4",
  "name": "webstudio-root",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:app": "react-router build",
    "build": "pnpm -r --filter='!./fixtures/*' build",
    "build:builder": "npx webstudio build --template docker && pnpm discardThem",
    "sync": "npx webstudio sync",
    "dts": "pnpm -r dts",
    "dev:app": "PORT=3030 react-router dev",
    "dev": "pnpm --filter='@webstudio-is/builder' dev",
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "fixtures": "pnpm -r fixtures:link && pnpm -r fixtures:sync && pnpm -r fixtures:build",
    "checks": "pnpm -r test && pnpm -r typecheck && pnpm lint && pnpm fixtures",
    "playwright": "pnpm -r playwright-init",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "migrations": "pnpm --filter=@webstudio-is/prisma-client generate && pnpm --filter=./packages/prisma-client migrations --dev --cwd ../../apps/builder",
    "build-figma-tokens": "cd packages/design-system && pnpm build-figma-tokens",
    "local:version-snapshot": "pnpm -r exec pnpm version prepatch --preid $(cat /dev/urandom | LC_ALL=C tr -dc 'a-z' | fold -w 8 | head -n 1)",
    "local:release": "pnpm build && pnpm dts && pnpm local:version-snapshot && pnpm local:publish-snapshot && pnpm local:dangerously-undo-version-snapshot && echo \"now execute\npnpm up -r -L '@webstudio-is/*' --registry http://localhost:4873\"",
    "typecheck": "tsc",
    "start:app": "PORT=3030 react-router-serve ./build/server/index.js",
    "discardThem": "git restore .dockerignore Dockerfile package.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@fontsource-variable/inter": "^5.0.20",
    "@fontsource-variable/manrope": "^5.0.20",
    "@fontsource/roboto-mono": "^5.0.18",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@storybook/addon-actions": "^8.6.4",
    "@storybook/addon-backgrounds": "^8.6.4",
    "@storybook/addon-controls": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.25",
    "@typescript/native-preview": "7.0.0-dev.20260120.1",
    "esbuild": "^0.25.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nano-staged": "^0.8.0",
    "prettier": "3.5.3",
    "react": "18.3.0-canary-14898b6a9-20240318",
    "react-dom": "18.3.0-canary-14898b6a9-20240318",
    "storybook": "^8.6.4",
    "tsx": "^4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": "^9.14.0",
    "yarn": "This project is configured to use pnpm"
  },
  "nano-staged": {
    "*.{ts,tsx,js,json,css,md}": "prettier --write"
  },
  "lint-staged": {
    "apps/devmat-cms/**/*": [
      "echo 'Skipping CMS changes'"
    ],
    "**/.webstudio/data.json": [
      "node scripts/fix-url.js"
    ],
    "**/app/__generated__/[home]._index.tsx": [
      "node scripts/fix-url.js"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "options": {
          "parser": "babel-ts"
        }
      }
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.2.70",
      "@types/react-dom": "^18.2.25",
      "react": "18.3.0-canary-14898b6a9-20240318",
      "react-dom": "18.3.0-canary-14898b6a9-20240318"
    },
    "patchedDependencies": {
      "@stitches/react@1.3.1-1": "patches/@stitches__react@1.3.1-1.patch",
      "@radix-ui/react-scroll-area@1.0.5": "patches/@radix-ui__react-scroll-area@1.0.5.patch",
      "@remix-run/dev": "patches/@remix-run__dev.patch"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@react-router/dev": "^7.5.3",
    "@react-router/fs-routes": "^7.5.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@webstudio-is/image": "0.238.0",
    "@webstudio-is/react-sdk": "0.238.0",
    "@webstudio-is/sdk": "0.238.0",
    "@webstudio-is/sdk-components-animation": "0.238.0",
    "@webstudio-is/sdk-components-react": "0.238.0",
    "@webstudio-is/sdk-components-react-radix": "0.238.0",
    "@webstudio-is/sdk-components-react-router": "0.238.0",
    "h3": "^1.15.1",
    "ipx": "^3.0.3",
    "isbot": "^5.1.25",
    "react": "18.3.0-canary-14898b6a9-20240318",
    "react-dom": "18.3.0-canary-14898b6a9-20240318",
    "react-router": "^7.5.3",
    "vite": "^6.3.4"
  }
}
