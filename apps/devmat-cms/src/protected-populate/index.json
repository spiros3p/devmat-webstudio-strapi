{
   "GET /api/general": {
      "content-type": "api::general.general",
      "populate": {
         "heroSection": {
            "fields": [
               "heroTitle",
               "heroSubTitle"
            ],
            "populate": {
               "leftButton": {
                  "fields": [
                     "title",
                     "href",
                     "target"
                  ]
               },
               "rightButton": {
                  "fields": [
                     "target",
                     "href",
                     "title"
                  ]
               },
               "heroMedia": {
                  "fields": [
                     "name",
                     "alternativeText",
                     "caption",
                     "width",
                     "height",
                     "formats",
                     "size",
                     "url",
                     "previewUrl",
                     "documentId"
                  ]
               }
            }
         },
         "customerLogos": {
            "fields": [
               "sectionTitle"
            ],
            "populate": {
               "customersLogos": {
                  "fields": [
                     "name",
                     "url"
                  ],
                  "populate": {
                     "logo": {
                        "fields": [
                           "name",
                           "alternativeText",
                           "caption",
                           "width",
                           "height",
                           "size",
                           "url",
                           "previewUrl",
                           "documentId"
                        ]
                     }
                  }
               }
            }
         },
         "benefitsSection": {
            "fields": [
               "title",
               "subtitle"
            ]
         },
         "gettingStartedSection": {
            "fields": [
               "title",
               "subtitle"
            ]
         },
         "solutions": {
            "fields": [
               "title",
               "subtitle"
            ]
         },
         "clientReviews": {
            "fields": [
               "title",
               "subtitle"
            ]
         },
         "faqs": {
            "fields": [
               "title",
               "subtitle"
            ]
         }
      },
      "fields": [
         "locale",
         "documentId"
      ]
   },
   "GET /api/benefits": {
      "content-type": "api::benefit.benefit",
      "populate": {
         "icon": {
            "fields": [
               "name",
               "id"
            ],
            "populate": {
               "icon": {
                  "fields": [
                     "name",
                     "alternativeText",
                     "caption",
                     "width",
                     "height",
                     "size",
                     "url",
                     "id"
                  ]
               }
            }
         },
         "image": {
            "fields": [
               "name",
               "alternativeText",
               "caption",
               "width",
               "height",
               "size",
               "url",
               "id"
            ]
         }
      },
      "fields": [
         "title",
         "subtitle",
         "locale",
         "id"
      ]
   },
   "GET /api/getting-started-steps": {
      "content-type": "api::getting-started-step.getting-started-step",
      "fields": [
         "index",
         "title",
         "subtitle",
         "locale",
         "id"
      ]
   },
   "GET /api/solutions": {
      "content-type": "api::solution.solution",
      "fields": [
         "title",
         "id",
         "locale",
         "subtitle"
      ],
      "populate": {
         "icon": {
            "fields": [
               "name",
               "id"
            ],
            "populate": {
               "icon": {
                  "fields": [
                     "name",
                     "alternativeText",
                     "caption",
                     "width",
                     "height",
                     "url",
                     "id"
                  ]
               }
            }
         }
      }
   },
   "GET /api/client-reviews": {
      "content-type": "api::client-review.client-review",
      "populate": {
         "icon": {
            "fields": [
               "name",
               "id"
            ],
            "populate": {
               "icon": {
                  "fields": [
                     "name",
                     "alternativeText",
                     "caption",
                     "width",
                     "height",
                     "formats",
                     "url",
                     "documentId",
                     "size"
                  ]
               }
            }
         }
      },
      "fields": [
         "name",
         "title",
         "message",
         "locale",
         "id"
      ]
   },
   "GET /api/site-state": {
      "content-type": "api::site-state.site-state",
      "fields": [
         "siteStatus"
      ]
   },
   "GET /api/faqs": {
      "content-type": "api::faq.faq",
      "fields": [
         "question",
         "id",
         "locale",
         "answer"
      ]
   }
}