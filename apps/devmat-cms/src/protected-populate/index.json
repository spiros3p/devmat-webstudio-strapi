{
   "GET /api/general": {
      "content-type": "api::general.general",
      "populate": {
         "heroSection": {
            "fields": [
               "heroTitle",
               "heroSubTitle"
            ],
            "populate": {
               "primaryButton": {
                  "fields": [
                     "title",
                     "href",
                     "target"
                  ]
               },
               "secondaryButton": {
                  "fields": [
                     "title",
                     "href",
                     "target"
                  ]
               },
               "heroMedia": {
                  "fields": [
                     "previewUrl",
                     "url",
                     "name",
                     "alternativeText",
                     "caption"
                  ],
                  "populate": {}
               }
            }
         },
         "customerLogos": {
            "fields": [
               "sectionTitle"
            ],
            "populate": {
               "customersLogos": {
                  "populate": {
                     "logo": {
                        "fields": [
                           "url",
                           "previewUrl",
                           "name",
                           "alternativeText",
                           "caption"
                        ]
                     }
                  }
               }
            }
         }
      }
   }
}